{
  "numbers.xml": {
    "/": {
      "xpaths": {
        "(8 * 2 + 1) = 17": true,
        "(1 + 8 * 2) = 17": true,
        "(7 - 3 + 1) = 5": true,
        "(8 - 4 + 5 - 6) = 3": true,
        "3 - 2 - 1": 0,
        "8 div 4 div 2": 1,
        "3 mod 7 mod 5": 3,
        "1 = 2 = 2": false,
        "2 != 3 != 1": false,
        "3 > 2 > 1": false,
        "3 >= 2 >= 2": false,
        "1 < 2 < 3": true,
        "2 <= 2 <= 3": true
      }
    }
  },
  "simple.xml": {
    "/": {
      "xpaths": {
        "string()": "abd"
      }
    },
    "/root": {
      "xpaths": {
        "string()": "abd"
      }
    },
    "/root/a": {
      "xpaths": {
        "string()": "a"
      }
    },
    "/root/c": {
      "xpaths": {
        "string()": "d"
      }
    }
  },
  "pi.xml": {
    "/": {
      "xpaths": {
        "//processing-instruction()": [
          "/foo[1]/processing-instruction(\"cheese\")[1]",
          "/foo[1]/bar[1]/processing-instruction(\"toast\")[1]",
          "/foo[1]/processing-instruction(\"cheese\")[2]"
        ],
        "//processing-instruction('cheese')": [
          "/foo[1]/processing-instruction(\"cheese\")[1]",
          "/foo[1]/processing-instruction(\"cheese\")[2]"
        ],
        "//processing-instruction('toast')": [
          "/foo[1]/bar[1]/processing-instruction(\"toast\")[1]"
        ],
        "string(//processing-instruction('toast'))": "is tasty"
      }
    }
  },
  "pi2.xml": {
    "/a/c": {
      "xpaths": {
        "//processing-instruction()": [
          "/a[1]/processing-instruction(\"toc\")[1]"
        ],
        "preceding-sibling::*" : [
          "/a[1]/b[1]"
        ],
        "preceding-sibling::node()": [
          "/a[1]/text()[1]",
          "/a[1]/b[1]",
          "/a[1]/text()[2]",
          "/a[1]/processing-instruction(\"toc\")[1]",
          "/a[1]/text()[3]"
        ],
        "preceding-sibling::processing-instruction()": [
          "/a[1]/processing-instruction(\"toc\")[1]"
        ],
        "string(preceding-sibling::processing-instruction())": "order-by=\"x\""
      }
    }
  },
  "id.xml" : {
    "/": {
      "xpaths": {
        "/foo/@id/parent::foo": [
          "/foo[1]"
        ]
      }
    },
    "/foo/@id": {
      "xpaths": {
        "parent::foo": [
          "/foo[1]"
        ]
      }
    }
  },
  "namespaces.xml": {
    "/": {
      "namespaces": {
        "foo": "http://fooNamespace/",
        "voo": "http://fooNamespace/",
        "bar": "http://barNamespace/",
        "alias": "http://fooNamespace/"
      },
      "xpaths": {
        "/*": [
          "/alias:a[1]"
        ],
        "string(/foo:a/b/c)": "Hello",
        "string(/foo:a/foo:d/foo:e)": "Hey",
        "string(/foo:a/alias:x/alias:y)": "Hey3",
        "string(/foo:a/foo:x/foo:y)": "Hey3"
      }
    }
  }
}